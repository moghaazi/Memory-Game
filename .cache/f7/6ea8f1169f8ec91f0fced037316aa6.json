ry-card {\r\n      width: 150px;\r\n      position: relative;\r\n      border-radius: 3px;\r\n      cursor: pointer;\r\n      transition: 0.2s;\r\n\r\n      .font,\r\n      .back {\r\n        position: absolute;\r\n        width: 100%;\r\n        height: 100%;\r\n        top: 0;\r\n        left: 0;\r\n        padding: 20px;\r\n        border-radius: 3px;\r\n        background: linear-gradient(to right, #ff9966, #ff5e62);\r\n      }\r\n\r\n      img {\r\n        width: 150px;\r\n        border-radius: 3px;\r\n      }\r\n    }\r\n\r\n    .flip .back {\r\n      opacity: 0;\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"dca855d0c9689b1fc87380e3ae032d13","cacheData":{"env":{}}}